{
  "name": "vv-logger-tiny",
  "version": "0.5.13",
  "description": "very-very simple logger for save logs to local files",
  "main": "index.js",
  "scripts": {
    "doc": "type .readme.md > readme.md && jsdoc2md app.js >> readme.md",
    "updatever": "npm version patch --no-git-tag-version",
