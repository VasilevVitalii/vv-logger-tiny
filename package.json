  "bugs": {
    "url": "https://github.com/VasilevVitalii/vv-logger-tiny/issues"
  },
  "homepage": "https://github.com/VasilevVitalii/vv-logger-tiny#readme",
  "devDependencies": {
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^8.2.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "vv-shared": "^3.0.14"
  }
}
